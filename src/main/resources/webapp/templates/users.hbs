{{#if this}}
    <table class="users table table-striped">
        <thead>
        <th>ID</th>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Membership</th>
        </thead>
        <tbody>
					{{#each this}}
          <tr>
              <td>{{id}}</td>
              <td>{{username}}</td>
              <td>{{firstName}}</td>
              <td>{{lastName}}</td>
              <td>{{membership}}</td>
          </tr>
					{{/each}}
        </tbody>
    </table>
{{else}}
    <p>No users found.</p>
{{/if}}